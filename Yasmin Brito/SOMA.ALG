Algoritmo "Soma"
Var
  num1, num2, num3, Soma: Real;

Inicio
    Escreva("Digite o primeiro número: ")
    Leia(num1)
    Escreva("Digite o segundo número: ")
    Leia(num2)
    Escreva("Digite o terceiro número: ")
    Leia(num3)
    
    //Somando os números
    Soma <- (num1 + num2 + num3)
    
    //Exibindo na tela o resultado
      Escreva("A soma dos números é: ", soma)
Fimalgoritmo