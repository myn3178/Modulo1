Algoritmo "Subtração"
Var
   num1, num2, resultado: Real

Inicio
    Escreva("Digite o primeiro número: ")
    Leia(num1)
    Escreva("Digite o segundo número: ")
    Leia(num2)
    
    //Calculando o resultado
    resultado <- (num1 - num2)
    
    //Exibindo o resultado na tela
    Escreva("O resultado da subtração é: ", resultado)
Fimalgoritmo