Algoritmo "Soma Real e Inteiro"
Var
   num1, num2, resultado: Real;
Inicio
   Escreva("Digite o primeiro número: ")
   Leia(num1)
   Escreva("Digite o segundo número: ")
   Leia(num2)
   
   //Somando os números
    resultado <- (num1 + num2)
    
    //Exibindo resultado
    
    Escreva("O resultado da Soma é: ", resultado)


Fimalgoritmo