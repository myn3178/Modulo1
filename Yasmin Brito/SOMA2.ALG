Algoritmo "Soma2"
Var
  num1, num2, resultado: Real;
Inicio
     Escreva("Digite o primeiro número: ")
     Leia(num1)
     Escreva("Digite o segundo número: ")
     Leia(num2)
     
     //Calculando a soma
     resultado <- (num1 + num2)
     
     //Exibindo o resultado
     
     Escreva("O resultado da soma é:",resultado)
Fimalgoritmo